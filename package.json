{
  "name": "electron-mcp-manager",
  "version": "1.0.0",
  "description": "A basic Electron application for MCP management",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "dist": "npm run build"
  },
  "keywords": [
    "electron",
    "mcp",
    "manager"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.example.electron-mcp-manager",
    "productName": "Electron MCP Manager",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "node_modules/**/*"
    ]
  }
}